<?xml version="1.0" encoding="utf-8" ?>
<objects xmlns="http://www.springframework.net">

  <object id="LibraryController"  type="Library.Controllers.LibraryController,Library" singleton="false">
    <property name="codeService" ref="CodeService" />
    <property name="libraryService" ref="LibraryService" />
  </object>

  <object id="CodeService" type="Library.Service.CodeService,Library.Service">
    <property name="codeDao" ref="CodeDao" />
  </object>

  <object id="LibraryService" type="Library.Service.LibraryService,Library.Service">
    <property name="libraryDao" ref="LibraryDao" />
  </object>

  <object id="CodeDao" type="Library.Dao.CodeDao,Library.Dao"/>
  <object id="LibraryDao" type="Library.Dao.LibraryDao,Library.Dao"/>

</objects>